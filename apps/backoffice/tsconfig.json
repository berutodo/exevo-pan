{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "types": ["@cloudflare/workers-types", "@types/jest", "@types/exevo-pan"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "test"]
}
